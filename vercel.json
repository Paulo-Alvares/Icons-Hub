{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/icons",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/icons",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/svgs",
      "dest": "/api/index.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
